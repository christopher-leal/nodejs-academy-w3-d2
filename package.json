    "debug": "^4.3.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mongoose-unique-validator": "^3.0.0",
    "morgan": "^1.10.0",
    "redis": "^4.0.1",
    "winston": "^3.3.3",
    "yargs": "^17.3.0"
