{
  "name": "nodejs-academy-w3-d2",
  "version": "1.0.0",
  "description": "NodeJS Academy code example. W3 d2",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "dev": "cross-env DEBUG=development:* NODE_ENV=development nodemon --exec ./node_modules/.bin/babel-node src/index.js --debug=console",
    "stage": "cross-env DEBUG=stage:* NODE_ENV=stage nodemon --exec babel-node src/index.js",
    "test": "cross-env nodemon --exec babel-node src/index.js",
    "start": "rm -rf node_modules dist && npm i && npm run build && cross-env NODE_ENV=production node dist/index.js --debug=winston"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/christopher-leal/nodejs-academy-w3-d2.git"
  },
